{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "启动",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/twinkle/main.go",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "调试",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/twinkle/main.go",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "调试udp",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/twinkle_udp_forward/main.go",
            "args": ["-udp","127.0.0.1:9001"],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "调试udp_client",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/twinkle_udp_forward/cmd/down.go",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "调试twinkle_watch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/twinkle_watch/main.go",
            "args": ["-d","./conf", "-r" ,"top"],
            "cwd": "${workspaceFolder}"
        },
    ]
}